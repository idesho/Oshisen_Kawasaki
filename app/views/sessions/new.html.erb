<div class="container ">
<h1>ログイン</h1>

<p id="notice"><%= notice %></p>


<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

<p><%= f.label :メールアドレス %></p>
<p><%= f.email_field :email %></p>

<p><%= f.label :パスワード %></p>
<p><%= f.password_field :password %></p>

<p><%= f.submit "ログイン" %></p>
<% end %>
</div>