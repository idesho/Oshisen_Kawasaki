<p id="notice"><%= notice %></p>
<h1>ofuros</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Introduction</th>
      <th>Address</th>
      <th>Prefecture</th>
      <th>Latitude</th>
      <th>Longitude</th>    

      <th colspan="3"></th>
    </tr>
  </thead>
    <% @ofuros.each do |ofuro| %>
      <tr>
        <td><%= ofuro.name %></td>
        <td><%= ofuro.introduction %></td>
        <td><%= ofuro.address %></td>
        <td><%= ofuro.latitude %></td>
        <td><%= ofuro.longitude %></td>
        

        <td><%= link_to 'Show', ofuro %></td>
        <td><%= link_to 'Edit', edit_ofuro_path(ofuro) %></td>
        <td><%= link_to 'Destroy', ofuro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New ofuro', new_ofuro_path %>