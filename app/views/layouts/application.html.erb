<!DOCTYPE html>
<html>
  <head>
    <title>sentou</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <% flash.each do |key, value| %>
      <%= content_tag(:div, value, class: "#{key}") %>
    <% end %>
    <%= link_to 'oshisen', root_path %>
    <% if logged_in? %>
      <%= link_to "マイページ", user_path(current_user.id) %>
      <%= link_to "ログアウト", session_path(current_user.id), method: :delete %>
    <% else %>
      <%= link_to "新規登録", new_user_path %>
      <%= link_to "ログイン", new_session_path %>
    <% end %>
      <%= link_to "管理者ページ", rails_admin_path %>
      <%= link_to 'ゲストユーザー', guest_path, method: :post, class: 'btn btn-success' %>
      <%= link_to 'ゲストユーザー(管理者)', guest_admin_path, method: :post %>
      <% if logged_in? %>
      <%= link_to '銭湯一覧', ofuros_path %>
          <% end %>
    <%= yield %>
  </body>
</html>